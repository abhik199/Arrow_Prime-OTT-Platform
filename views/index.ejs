  <!-- partial:index.partial.html -->

 
      <div class="logo text-center">
       
      </div>
      <div class="wrapper">
        <div class="inner-warpper text-center">
          <%
          if(errorMsg !== null && errorMsg !==''){
          %>
          <div class="alert alert-primary" class="alert"><%= errorMsg%></div>
          <%    
          }
          %>  

          <h2 class="title">Admin Login</h2>
          

          <form action="/" method="post"  id="formvalidate">
            <div class="input-group">
              <label class="palceholder" for="userName">User Name</label>
              <input class="form-control" name="username" id="userName" type="text" placeholder="" />
              <span class="lighting"></span>
            </div>
            <div class="input-group">
              <label class="palceholder" for="userPassword">Password</label>
              <input class="form-control" value="" name="password" id="userPassword" type="password" placeholder="" />
              <span class="lighting"></span>
            </div>

            <button type="submit"  id="login">Login</button>
            <div class="clearfix supporter">
              <div class="pull-left remember-me">
                <input value="" id="rememberMe" type="checkbox">
                <label for="rememberMe">Remember Me</label>
              </div>
              <a class="forgot pull-right" href="forgot_password">Forgot Password?</a>
            </div>
          </form>
        </div>
      </div>
     
        

