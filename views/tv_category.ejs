				<div class="card">
					<div class="card-header">
						<h4>Manage Tv Category</h4>
                        <button class="btn btn-success  float-right" ><a style="color: beige;" href="add_category">Add</a></button></div>
                    
					<div class="card-body">
						<div class="table-responsive">
                        <table class="table table-bordered">
                            <thead>
                              <tr>
                                <th scope="col">Sl.No</th>
                                <th scope="col">Category</th>
                                <th scope="col">Image</th>
                                <th scope="col" class="text-center">Action</th>
                            </tr>
                            </thead>
                            <tbody>
                                <% a.forEach((element,index)=>{%>
                                    <tr>
                                        <td>
                                            <%= index + 1%>
                                        </td>
                                        <td>
                                            <%=element.category_name%>
                                        </td>
                                        <td>
                                            <img style="width: 60px; height: 60px;" alt="image"
                                            src="<%=element.image_category%>">
                                        </td>
                                        
                                        <td>
											<a href="/tvupdate/<%=element._id%>"
                                            class="btn btn-danger"
                                            role="button">Edit</a>
                                            <a href="/tv_category/delete/<%=element._id%>"
                                            class="btn btn-danger" role="button">Delete</a>
                                        </td>
                                        
                                    </tr>

                                    <%})%>

                               
                            </tbody>
                          </table>

