	<div class="sidebar-overlay"></div>



	<div class="content-start transition">
		<div class="container-fluid dashboard">

			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<h4>Manage Series</h4>
                        <button class="btn btn-success  float-right" ><a style="color: beige;" href="add_series">Add Series</a></button>
					</div>
					<div class="card-body">
						<table class="table table-bordered">
                            <thead>
                              <tr>
								<th scope="col">Sl.No</th>
								<th scope="col">Name</th>
								
								<th scope="col">Category</th>
								<th scope="col">Thumbnailimage</th>
								<th scope="col">PosterImage</th>
								<!-- <th scope="col">Action</th> -->
								<th scope="col" class="text-center" >Action</th>
                              </tr>
                            </thead>
                            <tbody>
								<% a.forEach((element,index)=>{%>
									<tr>
										<!-- s.no -->
										<td><%= index + 1%></td>
										<td><%=element.title%></td>
										
										<!-- for Category -->
										<td><%=element.series_cat_id%></td>
										<td>
											<img style="width: 60px; height: 60px;" alt="image"
												src="<%=element.thumbnail_image%>"
										</td>
										
										<td>
											<img style="width: 60px; height: 60px;" alt="image"
												src="<%=element.poster%>">
										</td>
										
										<td><a href="edit_series/<%=element._id%>" class="btn btn-success" role="button">Edit</i></a>
											<a href="series/delete/<%=element._id%>" class="btn btn-success" role="button">Delete</i></a>
											<a class="btn btn-success" href="manage_session/<%=element._id%>">Manage Season</a>
											
                                        </td>
										
										
										
									</tr>
								
								<%})%>
                            </tbody>
                          </table>

						</div>
					</div>
				</div>
			</div>

		</div>
